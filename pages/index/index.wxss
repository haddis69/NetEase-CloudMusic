.banners{
  width: 100%;
  height: 300rpx;
}
.banners image{
  width: 100%;
  height: 100%;
}
.navContainer{
  display: flex;
}
.navItem{
  display: flex;
  flex-direction: column;
  width: 20%;
  align-items: center;
}
.navItem .iconfont{
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  border-radius: 50%;
  background-color: rgb(240, 19, 19);
  font-size: 50rpx;
  margin: 20rpx 0;
  color: white;
}
.navItem text{
  font-size: 26rpx;
}